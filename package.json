{
  "name": "@sketch-hq/sketch-file-format",
  "version": "3.2.1",
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "repository": "github:sketch-hq/sketch-file-format",
  "keywords": [
    "sketch",
    "sketch files",
    "file format",
    "file spec",
    "json",
    "json schema"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/json-schema": "7.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@sketch-hq/sketch-reference-files": "2.2.0",
    "ajv": "6.10.2",
    "chalk": "2.4.2",
    "change-case": "3.1.0",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "deepdash": "4.2.14",
    "globby": "10.0.1",
    "husky": "3.0.9",
    "js-yaml": "3.13.1",
    "json-schema-merge-allof": "0.6.0",
    "json-transforms": "1.1.2",
    "lodash": "4.17.15",
    "prettier": "1.18.2",
    "standard-version": "7.0.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "validate": "node --experimental-modules --no-warnings scripts/validate.mjs",
    "build": "node --experimental-modules --no-warnings scripts/build.mjs && yarn tsc",
    "watch": "node --experimental-modules --no-warnings scripts/watch.mjs",
    "integration-test": "node --experimental-modules --no-warnings scripts/integration-test.mjs",
    "test": "echo \"No test specified\"",
    "format-check": "prettier --check **/*.{mjs,md,yaml,json}",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version --no-verify",
    "prepublishOnly": "yarn build && yarn tsc",
    "commitlint": "commitlint --from HEAD~1 --to HEAD --verbose"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "yarn build && yarn validate && yarn integration-test && yarn test && yarn format-check"
    }
  }
}
