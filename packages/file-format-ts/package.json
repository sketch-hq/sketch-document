{
  "name": "@sketch-hq/sketch-file-format-ts",
  "description": "TypeScript types for the Sketch File Format",
  "version": "5.2.2",
  "main": "dist/cjs/index",
  "types": "dist/cjs/index",
  "module": "dist/esm/index",
  "license": "MIT",
